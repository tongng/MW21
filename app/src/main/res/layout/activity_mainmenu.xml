<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_bgmario01"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Mainmenu">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ic_bgmario01"
        android:orientation="vertical">


        <ImageView
            android:src="@drawable/ic_selectpng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnOK"

            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageButton
                android:src="@drawable/ic_icon_mp3"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:onClick="onButtonClick4"
                android:id="@+id/button4"></ImageButton>


            <ImageButton
                android:id="@+id/button51"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick51"
                android:src="@drawable/ic_logorock"></ImageButton>


            <ImageButton
                android:id="@+id/button7"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick7"
                android:src="@drawable/ic_coin"></ImageButton>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/button9"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:onClick="onButtonClick9"
                android:src="@drawable/ic_marksix_icon150"></ImageButton>


            <ImageButton
                android:id="@+id/button14"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick14"
                android:src="@drawable/ic_bmi"></ImageButton>


            <ImageButton
                android:id="@+id/button15"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:onClick="onButtonClick15"
                android:src="@drawable/ic_memo150"></ImageButton>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/button52"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick52"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/ic_sql"></ImageButton>


            <ImageButton
                android:id="@+id/button5"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:onClick="onButtonClick5"
                android:src="@drawable/ic_launcher_pc07"></ImageButton>


            <ImageButton
                android:id="@+id/button6"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:onClick="onButtonClick6"
                android:src="@drawable/ic_kfc"></ImageButton>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/button50"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick50"
                android:src="@drawable/ic_book"

                ></ImageButton>


            <ImageButton
                android:id="@+id/button8"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick8"
                android:src="@drawable/ic_mariologo"></ImageButton>


            <ImageButton
                android:id="@+id/button16"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light_transparent"
                android:onClick="onButtonClick16"
                android:src="@drawable/ic_icon_map"></ImageButton>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/button10"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:onClick="onButtonClick10"
                android:src="@drawable/ic_icon_video4"
                android:background="@android:drawable/screen_background_light_transparent"

                ></ImageButton>


            <ImageButton
                android:id="@+id/button11"
                android:layout_weight="1"
                android:onClick="onButtonClick11"
                android:src="@drawable/ic_up"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"></ImageButton>


            <ImageButton
                android:id="@+id/button12"
                android:onClick="onButtonClick12"
                android:layout_weight="1"
                android:src="@drawable/ic_contact"
                android:background="@android:drawable/screen_background_light_transparent"
                android:layout_width="100dp"
                android:layout_height="100dp"></ImageButton>


        </LinearLayout>


        <LinearLayout
            android:layout_marginTop="50dp"
            android:layout_width="match_parent"
            android:layout_height="22dp">


        </LinearLayout>


    </LinearLayout>
</RelativeLayout>